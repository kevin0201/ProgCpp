#ifndef OUVRAGE_H_
#define OUVRAGE_H_


class Ouvrage {
public:
	Ouvrage(const std::string& p_auteurs,
			const std::string& p_titre,
			const std::string& p_identifiant,
			int  p_annee,
			const std::string& p_ville,
			const std::string& p_editeur);
	int getAnnee() const;
	const std::string& getAuteurs() const;
	const std::string& getEditeur() const;
	const std::string& getIdentifiant() const;
	const std::string& getTitre() const;
	const std::string& getVille() const;

private:
	std::string m_auteurs;
	std::string m_editeur;
	std::string m_ville;
	std::string m_titre;
	int m_annee;
	std::string m_identifiant;
};

#endif /* OUVRAGE_H_ */
