#ifndef REFERENCEEXCEPTION_H_
#define REFERENCEEXCEPTION_H_

class ReferenceException : std::runtime_error{
public:
	ReferenceException();
	virtual ~ReferenceException();
};

class ReferenceDejaPresenteException : ReferenceException{
public:
	ReferenceDejaPresenteException();
	virtual ~ReferenceDejaPresenteException();
};

#endif /* REFERENCEEXCEPTION_H_ */
